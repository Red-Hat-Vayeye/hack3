# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET		/twitter		    		controllers.TwitterStreamController.index
GET		/twitter/keywords			controllers.TwitterKeyWordController.index
GET		/twitter/geolocation		controllers.TwitterGeolocationController.index
GET 	/keywords					controllers.WordClassifierController.index
GET 	/geolocation				controllers.GeolocationController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
